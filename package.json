  },
  "scripts": {
    "start": "NODE_ENV=development INDEX=1 webpack-dev-server --progress --colors",
    "start:current": "NODE_ENV=development SKETCHES=current webpack-dev-server --progress --colors",
    "start:dev": "npm-run-all --parallel start test:watch lint:watch",
    "start:dev:current": "npm-run-all --parallel start:current test:watch lint:watch",
    "build": "NODE_ENV=production webpack",
    "clean": "rimraf dist && mkdirp dist",
    "deploy": "gh-pages -d dist",
    "test": "mocha --colors --compilers js:babel-register --recursive",
    "test:watch": "npm run test -- --watch",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --color src test",
    "lint:watch": "onchange -p -v \"src/**/*.js\" \"test/**/*.js\" -- npm run lint"
  }
